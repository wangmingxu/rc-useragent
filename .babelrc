{
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": true,
        "targets": {
          "browsers": [
            "last 2 versions",
            "ios >= 9"
          ],
          "node": "current"
        },
        "modules": "commonjs",
        "debug": false,
        "useBuiltIns": "usage",
        "include": [],
        "exclude": []
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-react-jsx",
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    "@babel/plugin-proposal-class-properties"
  ]
}
